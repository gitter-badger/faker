#!/usr/bin/env node
var debug = require('debug')('faker');
var app = require('../app');
var domain = require('domain');
var srvDm = domain.create();
var server;

app.set('port', process.env.PORT || 80);

srvDm.run(function(){
	server = app.listen(app.get('port'), function() {
		debug('Yoooooooooooo' + server.address().port);
	});
});